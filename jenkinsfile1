pipeline {
   agent any
   
   stages {
       stage('Setup Parameter') {
           steps {
                  script {
                       properties([
                           parameters([
                                choice(
                                   choices: ["dev", "uat", "prod"],
                                   name: "Environment"
                                   )
                            ])
                       ])
                    }
               }
         } 
         stage('print parameter') {
              steps {
                 echo "$Environment"
                 }
          }
    }
}
                       
                                   
                   
